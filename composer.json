{
    "name": "ebs/pause-handler",
    "description": "this package is made for handling requests from one module to another whenever one of them is down",
    "type": "library",
    "license": "MIT",
     "authors": [
        {
            "name": "Braileanu Gabriel",
            "email": "gbraileanu15@gmail.com"
        }
    ],

    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Parents\\RequestPause\\": "src/"
        }
    },
    "require": {
        "php": "^7.2 || 8.1.*",
        "guzzlehttp/guzzle": "^7.4",
        "monolog/monolog": "<=2.7",
        "phpunit/phpunit": "^8.5 || 9.*",
        "laravel/framework": "6.19.1 || 7.*",
        "zendframework/zend-diactoros": "^2.2",
        "symfony/psr-http-message-bridge": "1.3",
        "nyholm/psr7": "^1.5",
        "symfony/cache": "^5.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parents\\RequestPause\\Providers\\ConfigServiceProvider",
                "Parents\\RequestPause\\Providers\\QueueServiceProvider"
            ]
        }
    }
}
